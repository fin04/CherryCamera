                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom|center_horizontal"
                android:layout_marginBottom="2dp"
                android:text="@string/scene_night" />
        </FrameLayout>

        <FrameLayout
            android:id="@+id/scnMenuLayout_4_1"
            android:layout_width="@dimen/MAIN_MENU_HEIGHT"
            android:layout_height="@dimen/MAIN_MENU_HEIGHT">

            <ImageView
                android:id="@+id/menu_image_4_1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:src="@drawable/icon_flowers" />

            <Button
                android:id="@+id/menu_button_4_1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:background="@drawable/selector_menu_scnbutton" />

            <TextView
                android:id="@+id/menu_text_4_1"
                style="@style/MenuScnButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom|center_horizontal"
                android:layout_marginBottom="2dp"
                android:text="@string/scene_flowers" />
        </FrameLayout>

        <FrameLayout
            android:id="@+id/scnMenuLayout_0_2"
            android:layout_width="@dimen/MAIN_MENU_HEIGHT"
            android:layout_height="@dimen/MAIN_MENU_HEIGHT" >

            <ImageView
                android:id="@+id/menu_image_0_2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:src="@drawable/icon_action" />

            <Button
                android:id="@+id/menu_button_0_2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:background="@drawable/selector_menu_scnbutton" />

            <TextView
                android:id="@+id/menu_text_0_2"
                style="@style/MenuScnButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom|center_horizontal"
                android:layout_marginBottom="2dp"
                android:text="@string/scene_sports" />
        </FrameLayout>

        <FrameLayout
            android:id="@+id/scnMenuLayout_1_2"
            android:layout_width="@dimen/MAIN_MENU_HEIGHT"
            android:layout_height="@dimen/MAIN_MENU_HEIGHT" >

            <ImageView
                android:id="@+id/menu_image_1_2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
